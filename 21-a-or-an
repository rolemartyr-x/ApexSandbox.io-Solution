public String aOrAn(String word) {
    Boolean startsWithA = word.startsWith('a');
    Boolean startsWithA = word.startsWith('e');
    Boolean startsWithA = word.startsWith('i');
    Boolean startsWithA = word.startsWith('o');
    Boolean startsWithA = word.startsWith('u');

    if(startsWithA == true) {
        String article = 'an ';
    } else {
        String article = 'a ';
    }

    String fullString = article + word;
    return fullString;
}