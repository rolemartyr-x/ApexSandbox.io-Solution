public String ageGroup(Integer n) {
    String groupDesc = '';
    if(ageGroup >= 0 && ageGroup <= 1) {
        groupDesc = 'Infant';
    } else if (ageGroup >= 2 && ageGroup <= 14) {
        groupDesc = 'Child';
    } else if (ageGroup >= 15 && ageGroup <= 21) {
        groupDesc = 'Youth';
    } else {
        groupDesc = 'Adult';
    }
    return groupDesc;
}