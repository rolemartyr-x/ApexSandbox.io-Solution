public String ageGroup(Integer n) {
    String groupDesc = '';
    if(n >= 0 && n <= 1) {
        groupDesc = 'Infant';
    } else if (n >= 2 && n <= 14) {
        groupDesc = 'Child';
    } else if (n >= 15 && n <= 21) {
        groupDesc = 'Youth';
    } else {
        groupDesc = 'Adult';
    }
    return groupDesc;
}